generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int     @id @default(autoincrement())
  login    String  @unique
  password String
  activ    Boolean @default(true)
}

model Opereytion {
  id         Int     @id @default(autoincrement())
  opereytion String  @unique
  result     String?
  sborka     Int?
  activ      Boolean @default(true)
}

model Grade {
  id      Int     @id @default(autoincrement())
  grade   String  @unique
  for     String?
  variant Int
  activ   Boolean @default(true)
}

model Collor {
  id      Int     @id @default(autoincrement())
  collor   String  @unique
  for     String?
  variant Int
  activ   Boolean @default(true)
}